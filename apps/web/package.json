{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "prebuild": "prisma generate",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "generate": "prisma generate",
    "generate:local": "dotenv -e .env.local -- prisma generate",
    "migrate": "dotenv -e .env.local -- prisma migrate dev",
    "migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "migrate:reset": "dotenv -e .env.local -- prisma migrate reset",
    "seed": "dotenv -e .env.local -- tsx prisma/seed.ts --environment=development",
    "seed:dev": "dotenv -e .env.local -- tsx prisma/seed.ts --environment=development",
    "seed:test": "dotenv -e .env.local -- tsx prisma/seed.ts --environment=test",
    "seed:prod": "dotenv -e .env.local -- tsx prisma/seed.ts --environment=production"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@langchain/core": "^0.3.68",
    "@langchain/google-genai": "^0.2.16",
    "@langchain/openai": "^0.6.7",
    "@prisma/client": "6.16.2",
    "@repo/ui": "workspace:*",
    "@tailwindcss/postcss": "^4.0.8",
    "@tanstack/react-query": "^5.82.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/html2pdf.js": "^0.10.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.1",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.12.1",
    "jspdf": "^3.0.3",
    "next": "^15.3.0",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "^0.4.4",
    "nextjs-toploader": "^3.8.16",
    "openai": "^5.12.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.0.1",
    "tailwind-variants": "^0.2.0",
    "tailwindcss": "^4.0.8",
    "tw-animate-css": "^1.2.4",
    "zod": "^3.25.75",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query-devtools": "^5.82.0",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.30.0",
    "prisma": "6.16.2",
    "tsx": "^4.20.3",
    "typescript": "5.8.2",
    "vitest": "^2.1.8"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts --environment=development"
  }
}
